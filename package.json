    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@types/multer": "^2.0.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "bcrypt": "^6.0.0",
