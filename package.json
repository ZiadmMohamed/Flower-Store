    "cross-env": "^10.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "globals": "^16.4.0",
    "ioredis": "^5.8.2",
    "mapped-types": "^0.0.1",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.0.1",
    "nodemailer": "^7.0.10",
